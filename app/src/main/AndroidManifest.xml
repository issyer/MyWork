<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kayll.myapplication">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/fo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/fo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label=" "
            android:theme="@style/ThemeFortel" />
        <activity
            android:name=".StartActivity"
            android:theme="@style/ImageTranslucentTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label=" "
            android:theme="@style/ImageTranslucentTheme" />
        <activity
            android:name=".RegisterActivity"
            android:label=" "
            android:theme="@style/ImageTranslucentTheme" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".PersonActivity"
            android:label="个人资料" />
        <activity
            android:name=".Face_Register_Activity"
            android:label=" "
            android:theme="@style/ThemeFortel2" />
        <activity
            android:name=".Person_name_Activity"
            android:label="修改姓名" />
        <activity
            android:name=".Person_age_Activity"
            android:label="修改年龄" />
        <activity
            android:name=".Person_sex_Activity"
            android:label="修改性别" />
        <activity
            android:name=".Person_addr_Activity"
            android:label="修改地址" />
    </application>

</manifest>